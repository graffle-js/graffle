{
  "name": "graffle",
  "description": "Simple GraphQL Client for JavaScript. Minimal. Extensible. Type Safe. Runs everywhere.",
  "version": "0.0.0-dripip",
  "packageManager": "pnpm@10.18.0",
  "type": "module",
  "bin": "cli.sh",
  "exports": {
    "./client": "./build/exports/client.js",
    "./extension": "./build/exports/extension.js",
    "./extension-exports": "./build/exports/extension_exports.js",
    "./generator": "./build/exports/generator.js",
    "./graffle": "./build/exports/graffle.js",
    ".": "./build/exports/index.js",
    "./kit": "./build/exports/kit.js",
    "./schema": "./build/exports/schema.js",
    "./utilities-for-generated": "./build/exports/utilities-for-generated.js",
    "./presets/bare": "./build/exports/presets/bare.js",
    "./presets/basic": "./build/exports/presets/basic.js",
    "./presets/minimal": "./build/exports/presets/minimal.js",
    "./generator-helpers/standard-scalar-types": "./build/exports/generator-helpers/standardScalarTypes.js",
    "./extensions/introspection": "./build/exports/extensions/introspection/runtime.js",
    "./extensions/document-builder/kit": "./build/exports/extensions/document-builder/kit.js",
    "./extensions/document-builder": "./build/exports/extensions/document-builder/runtime.js",
    "./extensions/throws": "./build/exports/extensions/throws/runtime.js",
    "./extensions/opentelemetry": "./build/exports/extensions/opentelemetry/runtime.js",
    "./extensions/schema-errors/gentime": "./build/exports/extensions/schema-errors/gentime.js",
    "./extensions/schema-errors": "./build/exports/extensions/schema-errors/runtime.js",
    "./extensions/transport-http": "./build/exports/extensions/transport-http/runtime.js",
    "./extensions/transport-memory": "./build/exports/extensions/transport-memory/runtime.js",
    "./extensions/upload": "./build/exports/extensions/upload/runtime.js"
  },
  "files": [
    "build",
    "src",
    "execute.sh"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/graffle-js/graffle.git"
  },
  "keywords": [
    "graphql",
    "request",
    "fetch",
    "graphql-client"
  ],
  "author": {
    "name": "Jason Kuhrt",
    "email": "jasonkuhrt@me.com",
    "url": "https://kuhrt.me"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/graffle-js/graffle/issues"
  },
  "homepage": "https://github.com/graffle-js/graffle",
  "scripts": {
    "extension:schema-errors:test:generate-fixtures": "rm -rf src/extensions/SchemaErrors/__tests__/fixture/graffle && pnpm graffle --project src/extensions/SchemaErrors/__tests__/fixture",
    "test:generate-schema-clients": "tsx tests/_/fixtures/schemas/generate-clients.ts",
    "graffle": "tsx ./src/cli/index.ts",
    "serve:pokemon": "tsx tests/_/services/pokemonManual.ts",
    "gen:graffle": "pnpm extension:schema-errors:test:generate-fixtures && pnpm website:gen:graffle",
    "examples:gen": "pnpm examples:gen:graffle && pnpm examples:gen:website",
    "examples:gen:graffle": "pnpm build && cd examples && pnpm i && pnpm gen:graffle",
    "examples:link-mode": "cd examples && pnpm add ..",
    "examples:test": "cd examples && pnpm test",
    "examples:gen:website": "tsx scripts/generate-examples-derivatives/generate.ts && pnpm fix:format",
    "examples:gen:outputs": "tsx scripts/generate-examples-derivatives/generate.ts --outputs",
    "examples:gen:tests": "tsx scripts/generate-examples-derivatives/generate.ts --tests && dprint fmt examples/__tests__/**/*",
    "website:gen:graffle": "pnpm build && cd website && pnpm i && pnpm gen:graffle",
    "fix:format": "dprint fmt",
    "fix:lint": "oxlint --fix",
    "check:examples:types": "cd examples && tsc --noEmit",
    "check:format": "dprint check",
    "check:lint": "oxlint",
    "check:publint": "tsx scripts/check-publint.ts",
    "prepublishOnly": "pnpm build",
    "dev": "pnpm build:watch:emit",
    "build:emit": "pnpm build --noCheck",
    "build:watch": "tsc --build tsconfig.build.json --watch",
    "build:watch:emit": "pnpm build:watch --noCheck",
    "build": "tsc --build tsconfig.build.json",
    "postbuild": "chmod +x ./build/cli/index.js",
    "test:unit": "vitest --dir src",
    "test:integration": "vitest --dir tests/integration",
    "test:e2e": "vitest --dir tests/e2e --testTimeout 30000 --run",
    "test": "vitest",
    "test:web": "vitest --environment jsdom",
    "test:types": "vitest --typecheck --dir src  --testNamePattern .*.test-d.ts",
    "test:coverage": "pnpm test -- --coverage",
    "release:stable": "dripip stable",
    "release:preview": "dripip preview",
    "release:pr": "dripip pr",
    "fixcheck": "pnpm run '/fix:.*/' && pnpm run '/check:.*/'",
    "fix": "pnpm run '/fix:.*/'",
    "release": "dripip stable",
    "build:clean": "pnpm tsc --build tsconfig.build.json --clean && rm -rf build",
    "prepack": "pnpm build",
    "check": "pnpm run '/check:.*/'",
    "prebuild": "pnpm build:clean",
    "check:types": "tsc",
    "ts:check:file": "tsc --noEmit",
    "ts:check:stats": "tsc --noEmit --extendedDiagnostics 2>&1 | grep -E '(Files|Lines|Identifiers|Symbols|Types|Instantiations|Time)'",
    "ts:check:trace": "tsc --noEmit --generateTrace .typescript-trace --incremental false",
    "ts:analyze": "npx @typescript/analyze-trace .typescript-trace"
  },
  "dependencies": {
    "@0no-co/graphql.web": "^1.0.12",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@molt/command": "^0.9.0",
    "@wollybeard/kit": "^0.78.3",
    "es-toolkit": "^1.35.0",
    "gql.tada": "^1.8.13",
    "is-plain-obj": "^4.1.0",
    "type-fest": "^4.40.0"
  },
  "peerDependencies": {
    "@dprint/formatter": "^0.4.0",
    "@dprint/typescript": "^0.93.0",
    "@opentelemetry/api": "^1.9.0",
    "graphql": "14 - 16",
    "prettier": "^3.3.3"
  },
  "peerDependenciesMeta": {
    "prettier": {
      "optional": true
    },
    "@opentelemetry/api": {
      "optional": true
    },
    "@dprint/formatter": {
      "optional": true
    },
    "@dprint/typescript": {
      "optional": true
    }
  },
  "devDependencies": {
    "@0no-co/graphqlsp": "^1.15.0",
    "@arethetypeswrong/cli": "^0.17.4",
    "@dprint/formatter": "^0.4.1",
    "@dprint/typescript": "^0.95.11",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/sdk-trace-base": "^1.30.0",
    "@opentelemetry/sdk-trace-node": "^1.30.0",
    "@pothos/core": "^4.6.0",
    "@pothos/plugin-simple-objects": "^4.1.1",
    "@pothos/plugin-zod": "^4.2.0",
    "@tsconfig/node-lts": "^22.0.1",
    "@tsconfig/node20": "^20.1.5",
    "@tsconfig/strictest": "^2.0.5",
    "@types/body-parser": "^1.19.5",
    "@types/express": "^5.0.1",
    "@types/node": "^22.14.1",
    "async-cleanup": "^1.0.0",
    "dprint": "^0.50.2",
    "dripip": "^0.10.0",
    "execa": "^9.5.2",
    "fs-jetpack": "^5.1.0",
    "get-port": "^7.1.0",
    "globby": "^14.1.0",
    "graphql": "^16.10.0",
    "graphql-scalars": "^1.24.2",
    "graphql-tag": "^2.12.6",
    "graphql-upload-minimal": "^1.6.1",
    "graphql-yoga": "^5.13.4",
    "jsdom": "^26.1.0",
    "memfs": "^4.17.0",
    "mermaid": "^11.12.0",
    "oxlint": "^1.18.0",
    "prettier": "^3.5.3",
    "publint": "^0.3.12",
    "strip-ansi": "^7.1.0",
    "tinyglobby": "^0.2.15",
    "tsx": "^4.19.3",
    "typescript": "^5.9.3",
    "vite-tsconfig-paths": "^5.1.4",
    "vitepress": "^1.6.3",
    "vitepress-plugin-mermaid": "^2.0.17",
    "vitest": "^3.1.1",
    "zod": "^3.24.3"
  },
  "imports": {
    "#src/*.js": "./build/*.js",
    "#src/*": "./build/*",
    "#graffle": "./build/exports/index.js",
    "#graffle/client": "./build/exports/client.js",
    "#graffle/extension": "./build/exports/extension.js",
    "#graffle/generator": "./build/exports/generator.js",
    "#graffle/kit": "./build/exports/kit.js",
    "#graffle/schema": "./build/exports/schema.js",
    "#graffle/utilities-for-generated": "./build/exports/utilities-for-generated.js",
    "#graffle/generator-helpers/standard-scalar-types": "./build/types/Schema/StandardTypes/scalar.js",
    "#graffle/extensions/transport-http": "./build/exports/extensions/transport-http/runtime.js",
    "#graffle/extensions/document-builder": "./build/exports/extensions/document-builder/runtime.js",
    "#graffle/extension-exports": "./build/exports/extension_exports.js",
    "#graffle/presets/bare": "./build/exports/presets/bare.js",
    "#graffle/presets/basic": "./build/exports/presets/basic.js",
    "#graffle/presets/minimal": "./build/exports/presets/minimal.js",
    "#lib/anyware": "./build/lib/anyware/$.js",
    "#lib/config-manager": "./build/lib/config-manager/$.js",
    "#lib/errors": "./build/lib/errors/$.js",
    "#lib/grafaid": "./build/lib/grafaid/$.js",
    "#lib/md-jsdoc": "./build/lib/md-jsdoc/$.js",
    "#lib/tex": "./build/lib/tex/$.js",
    "#lib/type-function": "./build/lib/type-function/$.js",
    "#test/schema/*": "./tests/_/fixtures/schemas/*",
    "#test/*.js": "./tests/_/*.js",
    "#test/*": "./tests/_/*"
  },
  "libra": {
    "entities": {
      "generated": [
        "website/pokemon",
        "website/graffle"
      ]
    }
  }
}
