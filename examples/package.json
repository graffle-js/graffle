{
  "name": "examples",
  "private": true,
  "type": "module",
  "scripts": {
    "test": "vitest --config vitest.config.ts",
    "check:types": "tsc --noEmit",
    "gen": "pnpm gen:graffle",
    "gen:graffle": "rm -rf ./$/graffle && pnpm graffle --defaultSchemaUrl http://localhost:3000/graphql --schema ../tests/_/fixtures/schemas/pokemon/schema.graphql --output ./$/graffle"
  },
  "dependencies": {
    "@opentelemetry/sdk-trace-base": "^1.30.0",
    "@opentelemetry/sdk-trace-node": "^1.30.0",
    "gql.tada": "^1.8.13",
    "graphql": "^16.10.0",
    "typescript": "^5.7.2"
  },
  "devDependencies": {
    "@tsconfig/node20": "^20.1.4",
    "@tsconfig/strictest": "^2.0.5",
    "@wollybeard/kit": "^0.71.0",
    "graffle": "file:..",
    "tsx": "^4.19.3",
    "vitest": "^3.1.1"
  },
  "pnpm": {
    "overrides": {
      "graffle": "file:.."
    }
  }
}
